{
	"luci-app-policy-routing": {
		"description": "Grant UCI and file access for luci-app-policy-routing",
		"read": {
			"cgi-io": [
				"exec"
			],
			"file": {
				"/usr/lib/opkg/status": [
					"read"
				],
				"/etc/init.d/policy-routing *": [
					"exec"
				],
				"/usr/bin/grep *": [
					"exec"
				],
				"/usr/sbin/grep *": [
					"exec"
				],
				"/usr/sbin/iptables *": [
					"exec"
				]
			},
			"uci": [
				"network",
				"policy-routing"
			]
		},
		"write": {
			"uci": [
				"policy-routing"
			]
		}
	}
}